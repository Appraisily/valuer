{
  "name": "invaluable-scraper",
  "version": "1.0.0",
  "description": "Enhanced scraper for Invaluable with resumable pagination and GCS storage",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "start:scraper": "node src/examples/invaluable-category-scraper.js",
    "test:gcs": "node src/examples/test-gcs-scraper.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scrape:furniture": "node src/scripts/resumable-scraper.js --category=furniture --maxRuntime=55 --batchSize=50",
    "scrape:art": "node src/scripts/resumable-scraper.js --category=fine-art --maxRuntime=55 --batchSize=50",
    "scrape:jewelry": "node src/scripts/resumable-scraper.js --category=jewelry --maxRuntime=55 --batchSize=50",
    "scrape:custom": "node src/scripts/resumable-scraper.js"
  },
  "dependencies": {
    "@google-cloud/storage": "^6.10.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "puppeteer": "^22.10.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "uuid": "^9.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "author": "",
  "license": "ISC"
}
