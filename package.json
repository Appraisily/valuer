{
  "name": "invaluable-scraper",
  "version": "1.0.0",
  "description": "Enhanced scraper for Invaluable with resumable pagination and GCS storage",
  "main": "src/examples/invaluable-category-scraper.js",
  "scripts": {
    "start": "node src/examples/invaluable-category-scraper.js",
    "start:dev": "NODE_ENV=development node src/examples/invaluable-category-scraper.js",
    "start:prod": "NODE_ENV=production node src/examples/invaluable-category-scraper.js",
    "test:gcs": "node src/examples/test-gcs-scraper.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:docker": "docker build -t invaluable-scraper .",
    "run:docker": "docker run -p 8080:8080 invaluable-scraper",
    "clean": "rm -rf node_modules package-lock.json temp"
  },
  "dependencies": {
    "@google-cloud/storage": "^6.12.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "puppeteer": "^21.5.2",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "uuid": "^9.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
